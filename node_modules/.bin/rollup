#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/reynald0sw/Downloads/Inventory Management System (1)/node_modules/rollup/dist/bin/node_modules:/proc/cygdrive/c/Users/reynald0sw/Downloads/Inventory Management System (1)/node_modules/rollup/dist/node_modules:/proc/cygdrive/c/Users/reynald0sw/Downloads/Inventory Management System (1)/node_modules/rollup/node_modules:/proc/cygdrive/c/Users/reynald0sw/Downloads/Inventory Management System (1)/node_modules:/proc/cygdrive/c/Users/reynald0sw/Downloads/node_modules:/proc/cygdrive/c/Users/reynald0sw/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/reynald0sw/Downloads/Inventory Management System (1)/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/reynald0sw/Downloads/Inventory Management System (1)/node_modules/rollup/dist/bin/node_modules:/proc/cygdrive/c/Users/reynald0sw/Downloads/Inventory Management System (1)/node_modules/rollup/dist/node_modules:/proc/cygdrive/c/Users/reynald0sw/Downloads/Inventory Management System (1)/node_modules/rollup/node_modules:/proc/cygdrive/c/Users/reynald0sw/Downloads/Inventory Management System (1)/node_modules:/proc/cygdrive/c/Users/reynald0sw/Downloads/node_modules:/proc/cygdrive/c/Users/reynald0sw/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/reynald0sw/Downloads/Inventory Management System (1)/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../rollup/dist/bin/rollup" "$@"
fi
